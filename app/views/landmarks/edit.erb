<h1>Edit Landmark</h1>
<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input type="hidden" id="hidden" name="_method" value="patch">
  <p>
    <label for="name">Landmark Name: </label>
    <input type="text" id="name" name="landmark[name]" value="<%= @landmark.name %>">
  </p>
  <p>
    <label for="year_completed">Year Completed: </label>
    <input type="number" id="year_completed" name="landmark[year_completed]"
    value="<%= @landmark.year_completed %>">
  </p>
  <p>
    <input type="submit" value="Edit Landmark">
  </p>
</form>
